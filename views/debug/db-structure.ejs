<div class="row">
  <div class="col-12">
    <div class="d-flex justify-content-between align-items-center mb-4">
      <h1>Cấu trúc Database</h1>
      <a href="/dashboard" class="btn btn-secondary">Quay lại</a>
    </div>
  </div>
</div>

<div class="row">
  <div class="col-12">
    <div class="card">
      <div class="card-body">
        <% tables.forEach(table => { %>
          <div class="mb-4">
            <h4 class="border-bottom pb-2"><%= table.table %></h4>
            <pre class="bg-light p-3 rounded"><%= table.structure %></pre>
          </div>
        <% }); %>
      </div>
    </div>
  </div>
</div> 